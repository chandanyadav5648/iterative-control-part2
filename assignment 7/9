//armstrong number or not
#include<stdio.h>
int main()
{
     int  r , sum , temp , num ;//temp is a variable

    // printf("enter a number to check armstrong number or not");
    // scanf("%d",&n);

     for(int i = 1 ; i<=1000 ; i++)
     {  temp = i;
        num = i;
        sum =0;

     while(num)
     {
         r = num%10;
         sum = sum + (r*r*r);

         num = num/10;
     }

     if(temp == sum)
        printf("%d\t",sum);


     }

     return 0;
}
