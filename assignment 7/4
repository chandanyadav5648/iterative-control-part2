//write a program hcf of two numbers
#include<stdio.h>
int main()
{
    int a , b , lcm , hcf ;
    printf("enter two numbers for hcf");
    scanf("%d%d",&a,&b);
    printf("a=%d,b=%d",a,b);

    for(int i=1 ; i<=a*b ;i++)
    {
        if((i%a==0) && (i%b==0))
        { lcm=i;
            break;
        }
    }

    hcf =(a * b) / lcm ;

    printf("\n%d",hcf);


    return 0;
}

